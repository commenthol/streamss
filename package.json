{
  "name": "streamss",
  "version": "1.0.0",
  "description": "stream2 event streams",
  "keywords": [
    "event-streams",
    "stream2"
  ],
  "homepage": "https://github.com/commenthol/streamss",
  "bugs": {
    "url": "https://github.com/commenthol/streamss/issues"
  },
  "repository": {
    "type": "git",
    "url": "github.com/commenthol/streamss"
  },
  "license": "MIT",
  "author": "commenthol <commenthol@gmail.com>",
  "main": "index.js",
  "directories": {
    "lib": "lib",
    "example": "examples",
    "test": "test"
  },
  "scripts": {
    "all": "npm run clean && npm run test",
    "clean": "rimraf doc coverage",
    "clean:all": "npm run clean && rimraf node_modules *.tgz",
    "coverage": "nyc -r text -r html npm test",
    "doc": "jsdoc -d doc -c jsdoc.json index.js lib/*.js",
    "lint": "eslint '**/*.js'",
    "prepublishOnly": "npm run all",
    "test": "mocha"
  },
  "eslintConfig": {
    "extends": "standard",
    "plugins": [
      "standard"
    ],
    "rules": {
      "key-spacing": 0
    }
  },
  "dependencies": {
    "lodash.omit": "^4.5.0",
    "streamss-through": "^1.0.1"
  },
  "devDependencies": {
    "eslint": "^5.3.0",
    "eslint-config-standard": "^12.0.0",
    "eslint-plugin-import": "^2.17.2",
    "eslint-plugin-node": "^9.0.1",
    "eslint-plugin-promise": "^4.1.1",
    "eslint-plugin-standard": "^4.0.0",
    "mocha": "^6.1.4",
    "rimraf": "^2.6.3"
  },
  "engines": {
    "node": ">=4.5.0"
  },
  "maintainers": [
    "commenthol <commenthol@gmail.com>"
  ]
}
