{
  "name": "streamss",
  "description": "stream2 event streams",
  "version": "0.2.4",
  "main": "index.js",
  "engines": {
    "node": ">=0.8.0"
  },
  "repository": {
    "type": "git",
    "url": "github.com/commenthol/streamss"
  },
  "keywords": [
    "stream2",
    "event-streams"
  ],
  "author": "commenthol <commenthol@gmail.com>",
  "maintainers": "commenthol <commenthol@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/commenthol/streamss/issues"
  },
  "homepage": "https://github.com/commenthol/streamss",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "test": "mocha test/*.mocha.js",
    "cover": "istanbul cover _mocha --report lcov --report text -- -R dot --check-leaks test/*.mocha.js",
    "lint": "eslint '**/*.js'",
    "doc": "jsdoc -d doc -c jsdoc.json index.js lib/*.js",
    "clean": "rimraf doc coverage"
  },
  "dependencies": {
    "streamss-shim": "~0.0.2",
    "streamss-through": "~0.2.1",
    "underscore": "~1.8.3"
  },
  "devDependencies": {
    "mocha": "^3.2.0",
    "rimraf": "^2.5.4"
  },
  "eslintConfig": {
    "extends": "standard",
    "plugins": [
      "standard"
    ],
    "rules": {
      "key-spacing": 0
    }
  }
}
